extends layout

block content
    h3 Run command
    div(class="container-fluid")
        form(class="form-horizontal")
            div(class="form-group")
                input(type="text", class="form-control", placeholder="Enter the command, parameters separated by a space.", name="cmd", list="cmd_list")
                datalist(id="cmd_list")
                    each cmd in cmdList
                        option(value="#{cmd}")
            button(type="submit", class=["btn", "btn-primary"], id="submit_cmd") Submit

    div(class="clearfix")

    h3 Output
    div(id="cmd_output")
